<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Advertisement" table="advertisement">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="advertisement_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="description" type="text" column="description" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="price" type="text" column="price" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="links" type="text" column="links" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
